{
  "defaultSeverity": "error",
  "extends": [
      "tslint:latest",
      "tslint-eslint-rules",
      "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": [true, { "singleQuote": true }],
    "interface-name": [true, "never-prefix"],
    "curly": [true, "ignore-same-line"],
    "array-type": false,
    "object-literal-sort-keys": false
  },
  "jsRules": {}
}
